{
  "_class": ["HyperParms", "List", "Opts"],
  "name": "Spline-IpGp",
  "list": [
    {
      "_class": ["Trajs", "HyperParms", "Opts"],
      "normalize": "meanAndCov",
      "initialState": {
        "_class": ["FromObs", "InitialState", "Opts"]
      },
      "fitTrajs": {
        "_class": ["InterpolationSpline", "FitTrajs", "Opts"],
        "target": "point",
        "interSteps": 5
      },
      "derivMethod": "none",
      "derivFun": {
        "_class": ["GaussianProcess", "DerivFun", "Opts"],
        "neighbors": 50,
        "bandwidth": 10,
        "regulation": 1e-9
      }
    }
  ]
}
