{
  "_class": ["HyperParms", "List", "Opts"],
  "name": "Spline-ThreshLm",
  "list": [
    {
      "_class": ["Trajs", "HyperParms", "Opts"],
      "normalize": "scale",
      "initialState": {
        "_class": ["FromObs", "InitialState", "Opts"]
      },
      "fitTrajs": {
        "_class": ["InterpolationSpline", "FitTrajs", "Opts"],
        "target": "point",
        "interSteps": 1
      },
      "derivMethod": "none",
      "derivFun": {
        "_class": ["ThreshLm", "DerivFun", "Opts"],
        "features": "poly5",
        "threshold": 0.1,
        "iterations": 10
      }
    }
  ]
}
