{
  "_class": ["HyperParms", "List", "Opts"],
  "name": "Spline-Poly2",
  "list": [
    {
      "_class": ["Trajs", "HyperParms", "Opts"],
      "normalize": "meanAndCov",
      "initialState": {
        "_class": ["FromObs", "InitialState", "Opts"]
      },
      "fitTrajs": {
        "_class": ["InterpolationSpline", "FitTrajs", "Opts"],
        "target": "point",
        "interSteps": 10
      },
      "derivMethod": "none",
      "derivFun": {
        "_class": ["GlobalLm", "DerivFun", "Opts"],
        "features": {
            "z": ["1", "x", "x1*x2", "x^2"]
        }
      }
    }
  ]
}
