{
  "_class": ["HyperParms", "List", "Opts"],
  "name": "Spline-True",
  "list": [
    {
      "_class": ["Trajs", "HyperParms", "Opts"],
      "normalize": "none",
      "initialState": {
        "_class": ["FromObs", "InitialState", "Opts"]
      },
      "fitTrajs": {
        "_class": ["InterpolationSpline", "FitTrajs", "Opts"],
        "target": "point",
        "interSteps": 10
      },
      "derivMethod": "none",
      "derivFun": {
        "_class": ["GlobalLm", "DerivFun", "Opts"],
        "features": {
            "z1": "x2-x1", 
            "z2": "x1",
            "z3": "x3"
        },
        "transform": {
            "z1": "y1",
            "z2": "y2 + x2 + x1*x3",
            "z3": "y3 - x1*x2"
        },
        "invTransform": {
            "y1": "z1",
            "y2": "z2 - x2 - x1*x3",
            "y3": "z3 + x1*x2"
        }
      }
    }
  ]
}
